<h1>Shop\User</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>                <td>{{ entity.id }}</td>        </tr>
        <tr>
            <th>Firstname</th>                <td>{{ entity.firstName }}</td>        </tr>
        <tr>
            <th>Lastname</th>                <td>{{ entity.lastName }}</td>        </tr>
        <tr>
            <th>Email</th>                <td>{{ entity.email }}</td>        </tr>
        <tr>
            <th>Address</th>                <td>{{ entity.address }}</td>        </tr>
        <tr>
            <th>Isenabled</th>                <td>{{ entity.isEnabled }}</td>        </tr>
        <tr>
            <th>Permissiontype</th>                <td>{{ entity.permissionType }}</td>        </tr>
        <tr>
            <th>Createdat</th>                <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>        </tr>
        <tr>
            <th>Modifiedat</th>                <td>{{ entity.modifiedAt|date('Y-m-d H:i:s') }}</td>        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('crud_user') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('crud_user_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('crud_user_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
