<h1>Shop\Image</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>                <td>{{ entity.id }}</td>        </tr>
        <tr>
            <th>Name</th>                <td>{{ entity.name }}</td>        </tr>
        <tr>
            <th>Description</th>                <td>{{ entity.description }}</td>        </tr>
        <tr>
            <th>Createdat</th>                <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>        </tr>
        <tr>
            <th>Modifiedat</th>                <td>{{ entity.modifiedAt|date('Y-m-d H:i:s') }}</td>        </tr>
        <tr>
            <th>Size</th>                <td>{{ entity.size }}</td>        </tr>
        <tr>
            <th>Image</th>                <td>{{ entity.image }}</td>        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('crud_image') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('crud_image_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('crud_image_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
