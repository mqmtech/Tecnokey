<h1>Shop\Order</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Description</th>
            <td>{{ entity.description }}</td>
        </tr>
        <tr>
            <th>Createdat</th>
            <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Modifiedat</th>
            <td>{{ entity.modifiedAt|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Checkoutat</th>
            <td>{{ entity.checkoutAt|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Shippingbaseprice</th>
            <td>{{ entity.shippingBasePrice }}</td>
        </tr>
        <tr>
            <th>Totalproductsbaseprice</th>
            <td>{{ entity.totalProductsBasePrice }}</td>
        </tr>
        <tr>
            <th>Totalbaseprice</th>
            <td>{{ entity.totalBasePrice }}</td>
        </tr>
        <tr>
            <th>Tax</th>
            <td>{{ entity.tax }}</td>
        </tr>
        <tr>
            <th>Taxprice</th>
            <td>{{ entity.taxPrice }}</td>
        </tr>
        <tr>
            <th>Totalprice</th>
            <td>{{ entity.totalPrice }}</td>
        </tr>
        <tr>
            <th>Shippingmethod</th>
            <td>{{ entity.shippingMethod }}</td>
        </tr>
        <tr>
            <th>Orderstate</th>
            <td>{{ entity.orderState }}</td>
        </tr>
        <tr>
            <th>Status</th>
            <td>{{ entity.status }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('crud_order') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('crud_order_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('crud_order_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
